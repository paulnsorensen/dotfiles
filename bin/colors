#!/usr/bin/env bash
# Print Selenized Deutan Warm color palette swatches

echo ""
echo "═══════════════════════════════════════════════"
echo "      SELENIZED DEUTAN WARM COLOR PALETTE      "
echo "═══════════════════════════════════════════════"
echo ""
echo "Background / Foreground"
echo "───────────────────────"
printf "BG         \e[48;2;62;53;48m      \e[0m #3E3530  L*=23\n"
printf "BG_ALT     \e[48;2;74;64;59m      \e[0m #4A403B  L*=28\n"
printf "BG_HL      \e[48;2;93;83;77m      \e[0m #5D534D  L*=36\n"
printf "DIM        \e[48;2;144;133;121m      \e[0m #908579  L*=56 (comments)\n"
printf "FG         \e[48;2;196;183;163m      \e[0m #C4B7A3  L*=75\n"
printf "FG_BRIGHT  \e[48;2;224;211;190m      \e[0m #E0D3BE  L*=85\n"
echo ""
echo "Accent Colors (Deuteranopia-safe)"
echo "──────────────────────────────────"
printf "RED        \e[48;2;240;90;25m      \e[0m #F05A19  Vermillion (errors)\n"
printf "GREEN      \e[48;2;62;184;165m      \e[0m #3EB8A5  Teal (success)\n"
printf "YELLOW     \e[48;2;240;184;34m      \e[0m #F0B822  Types, highlights\n"
printf "BLUE       \e[48;2;0;145;239m      \e[0m #0091EF  Functions\n"
printf "MAGENTA    \e[48;2;252;148;216m      \e[0m #FC94D8  Constants\n"
printf "CYAN       \e[48;2;58;203;242m      \e[0m #3ACBF2  Strings\n"
printf "ORANGE     \e[48;2;255;135;82m      \e[0m #FF8752  Warnings\n"
printf "VIOLET     \e[48;2;181;135;227m      \e[0m #B587E3  Keywords\n"
echo ""
echo "Bright Variants"
echo "───────────────"
printf "BR_RED     \e[48;2;255;105;40m      \e[0m #FF6928\n"
printf "BR_GREEN   \e[48;2;79;198;179m      \e[0m #4FC6B3\n"
printf "BR_YELLOW  \e[48;2;255;199;52m      \e[0m #FFC734\n"
printf "BR_BLUE    \e[48;2;0;159;254m      \e[0m #009FFE\n"
printf "BR_MAGENTA \e[48;2;255;162;231m      \e[0m #FFA2E7\n"
printf "BR_CYAN    \e[48;2;79;217;255m      \e[0m #4FD9FF\n"
printf "BR_ORANGE  \e[48;2;255;149;95m      \e[0m #FF955F\n"
printf "BR_VIOLET  \e[48;2;196;149;242m      \e[0m #C495F2\n"
echo ""
echo "ANSI 16-color strip:"
printf "\e[48;2;62;53;48m  \e[48;2;240;90;25m  \e[48;2;62;184;165m  \e[48;2;240;184;34m  \e[48;2;0;145;239m  \e[48;2;252;148;216m  \e[48;2;58;203;242m  \e[48;2;196;183;163m  \e[0m Normal 0-7\n"
printf "\e[48;2;144;133;121m  \e[48;2;255;105;40m  \e[48;2;79;198;179m  \e[48;2;255;199;52m  \e[48;2;0;159;254m  \e[48;2;255;162;231m  \e[48;2;79;217;255m  \e[48;2;224;211;190m  \e[0m Bright 8-15\n"
echo ""
echo "Semantic mapping:"
echo "  Error   → Vermillion (not red)"
echo "  Success → Teal (not green)"
echo "  Warning → Orange"
echo "  Info    → Cyan"
echo ""
